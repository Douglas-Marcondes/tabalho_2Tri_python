setor_id = 1
nome = input("Nome do setor: ")
responsavel = input("ID do responsável: ")
area = input("Área de cobertura: ")
horario = input("Horário (ex: 08:00-18:00): ")

print("\n=== DADOS DO SETOR ===")
print(f"ID: {setor_id}")
print(f"Nome: {nome}")
print(f"Responsável: {responsavel}")
print(f"Área: {area}")
print(f"Horário: {horario}")
